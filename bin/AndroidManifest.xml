    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.minal.wtcaccessories.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.minal.wtcaccessories.TimeConnectionActivity"
            android:label="@string/title_activity_time_connection" >
        </activity>
        <activity
            android:name="com.minal.wtcaccessories.AmeiActivity"
            android:label="@string/title_activity_amei" >
        </activity>
        <activity
            android:name="com.minal.wtcaccessories.CasioActivity"
            android:label="@string/title_activity_casio" >
        </activity>
    </application>

</manifest>
